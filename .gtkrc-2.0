
style "panel"
{
fg[NORMAL]               = "#ffffff"
fg[PRELIGHT]            = "#000000"
fg[ACTIVE]               = "#ffffff"
fg[SELECTED]            = "#000000"
fg[INSENSITIVE]            = "#8A857C"

text[NORMAL]            = "#161616"
text[PRELIGHT]            = "#000000"
text[ACTIVE]               = "#000000"
text[SELECTED]            = "#ffffff"
text[INSENSITIVE]            = "#8A857C"
}
widget "*PanelWidget*" style "panel"
widget "*PanelApplet*" style "panel"
class "*Panel*" style "panel"

gtk-menu-popup-delay = 0
